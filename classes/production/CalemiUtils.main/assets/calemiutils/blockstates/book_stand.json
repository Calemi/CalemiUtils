{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "calemiutils:book_stand"
      }
    },
    {
      "when": {
        "facing": "north",
        "book": "true"
      },
      "apply": {
        "model": "calemiutils:book"
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "calemiutils:book_stand",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "south",
        "book": "true"
      },
      "apply": {
        "model": "calemiutils:book",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "calemiutils:book_stand",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "west",
        "book": "true"
      },
      "apply": {
        "model": "calemiutils:book",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "calemiutils:book_stand",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "east",
        "book": "true"
      },
      "apply": {
        "model": "calemiutils:book",
        "y": 90
      }
    }
  ]
}
